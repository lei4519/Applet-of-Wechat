<view class='slider'>
  <swiper indicator-dots='true' autoplay='true' circular='true'>
    <swiper-item wx:for="{{  swiperList }}" wx:key="{{ item.id }}">
      <image src='{{ item.image }}'></image>
    </swiper-item>
  </swiper>
</view>
<view class='index-nav'>
  <navigator class='nav-item bdr' url='/pages/shopList/shopList?id={{ item.id }}' wx:for="{{ categoryList }}" wx:key="{{item.id}}">
    <image src='{{ item.icon }}'></image>
    <text>{{ item.name }}</text>
  </navigator>
</view>
<view class='divider'></view>
<view class='index-enter'>
  <navigator class='enter-item'>
    <image src='/assets/images/link-01.png'></image>
  </navigator>
  <navigator class='enter-item'>
    <image src='/assets/images/link-02.png'></image>
  </navigator>
</view>